# Parent class
class Vehicle:
    def move(self):
        pass  # To be overridden by child classes

# Subclasses implementing different move() behaviors
class Car(Vehicle):
    def move(self):
        return "Driving ğŸš—"

class Plane(Vehicle):
    def move(self):
        return "Flying âœˆï¸"

class Boat(Vehicle):
    def move(self):
        return "Sailing ğŸš¢"

# Example usage
vehicles = [Car(), Plane(), Boat()]

for vehicle in vehicles:
    print(vehicle.move())
